{
  "name": "trello-api-proxy-njs",
  "version": "1.0.0",
  "description": "Trello API Proxy - Node.js implementation that returns markdown formatted data for LLM consumption",
  "main": "trello-proxy.js",
  "type": "module",
  "scripts": {
    "start": "node trello-proxy.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "moduleFileExtensions": ["js"],
    "testMatch": ["**/*.test.js"]
  }
}